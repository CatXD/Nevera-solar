.................... /////////////////////////////////////////////////////////////////////////////////////////// 
.................... // Devuelve el carácter situado en la posición x,y de la DDRAM 
.................... /////////////////////////////////////////////////////////////////////////////////////////// 
....................  
.................... char lcd_getc( BYTE x, BYTE y) { 
....................    char value; 
....................  
....................     lcd_gotoxy(x,y); 
....................     while ( bit_test(lcd_read_byte(),7) ); // espera a que el flag de ocupado esté a 0 
....................     lcd.rs=1; 
....................     value = lcd_read_byte(); 
....................     lcd.rs=0; 
....................     return(value); 
.................... } 
