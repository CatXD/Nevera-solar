.................... //////////////////////////////////////////////////////////////////////////////////// 
.................... // Limpia la linea correspondiente y se situa al principio de la misma            // 
.................... //////////////////////////////////////////////////////////////////////////////////// 
.................... void lcd_clr_line(char fila) 
.................... { 
....................    int j; 
....................  
....................    lcd_gotoxy(1,fila); 
....................     for (j=0;j<40;j++) lcd_putc(' '); 
....................  
....................     lcd_gotoxy(1,fila); 
.................... } 
